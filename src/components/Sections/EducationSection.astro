---
import EducationCard from "@/components/Cards/ResumeCard.astro";
const educationList = [
  {
    time: "2023 - Present",
    degree: "Computer System Technology Diploma",
    institution: "British Columbia Institute of Technology (BCIT)",
  },
  {
    time: "2017 - 2021",
    degree: "Bachelor's of Psychology",
    institution: "The University of British Columbia (UBC)",
  },
];
---

{
  educationList.map((education) => {
    return (
      <EducationCard
        timespan={education.time}
        title={education.degree}
        organization={education.institution}
      />
    );
  })
}
